    "esbuild-jest": "^0.5.0",
    "husky": "^9.0.11",
    "jest": "^29.0.3",
    "obsidian": "^1.4.11",
    "prettier": "^2.7.1",
    "tslib": "^2.4.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ts-deepmerge": "^3.0.0"
